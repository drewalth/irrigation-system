datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model TimeSlot {
  id        Int @id @default(autoincrement())
  active    Boolean @default(false)
  startTime DateTime
  endTime   DateTime
  createdAt DateTime? @default(now())
  updatedAt DateTime?
}

model Sensor {
 id         Int @id @default(autoincrement())
 sensorType String
}
